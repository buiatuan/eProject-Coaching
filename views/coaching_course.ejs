<script type="text/javascript">
    var a = 0;
    function show_hide(){
        console.log("la");
        if(a==0)
        {
            document.getElementById("show").style.display="inline";
            return a=1;
        }
        else {
            document.getElementById("show").style.display="none";
            return a=0;
        }
    }
   // x√≥a checked
    function resetAll(){
        document.getElementById("inlineCheckbox1").checked = false;
        document.getElementById("inlineCheckbox2").checked = false;
        document.getElementById("inlineCheckbox3").checked = false;
        document.getElementById("clearInput").value = "";
        document.getElementById("clearInput1").value = "";
        document.getElementById("togBtn").checked = false;
        document.getElementById("clearInput3").value = "";

    }

</script>
